@model SimpleCMS.Admin.Models.ViewModel.UserViewModel
@{
    ViewData["Title"] = "Index";
}


<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(user => user.Users![0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(user => user.Users![0].Email)
            </th>
          
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Users)
        {
                <tr>
                    <td>
                    @Html.DisplayFor(userItem => item.Name)
                    </td>
                    <td>
                    @Html.DisplayFor(userItem => item.Email)
                    </td>
                </tr>
        }
    </tbody>
</table>
